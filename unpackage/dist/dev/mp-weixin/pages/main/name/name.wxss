	justify-content: center;
	align-items: center;
}
.userAvatar image{
	width: 80rpx;
	height: 80rpx;
}
.userInfo{
	display: flex;
	flex-direction: column;
}
/* 登录弹窗 */
.showLogin {
	display: flex;
	width: 600rpx;
	justify-content: center;
	align-items: center;
	flex-direction: column;
	padding: 60rpx 30rpx;
