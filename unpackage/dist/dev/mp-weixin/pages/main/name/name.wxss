.viewBox {
	background-color: #fff;
	width: 90%;
	display: flex;
	flex-direction: column;
	padding: 30rpx;
	margin: 30rpx auto 20rpx auto;
	border-radius: 10rpx;
}
.viewGroup {
	display: flex;
