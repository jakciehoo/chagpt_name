}
.viewList {
	display: flex;
	flex-direction: column;
}
.viewItems {
	display: flex;
	flex-direction: row;
	align-items: center;
	flex-wrap: wrap;
}
.viewItem {
	display: flex;
	justify-content: center;
	align-items: center;
}
.xing {
	width: 50rpx;
	height: 50rpx;
}
.gezi {
	width: 90rpx;
	height: 90rpx;
	position: relative;
	display: flex;
	justify-content: center;
	align-items: center;
	margin-right: 20rpx;
}
.gezi image {
	width: 90rpx;
	height: 90rpx;
}
.zi {
	position: absolute;
	z-index: 10000;
